{
  "name": "yx-plus",
  "version": "1.0.2-5",
  "private": false,
  "keywords": [
    "yx",
    "yx-ui",
    "ui",
    "vue",
    "frontend",
    "components"
  ],
  "files": [
    "lib",
    "src",
    "packages",
    "types"
  ],
  "description": "A Component Library for Vue.js.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix --ext .js,.vue examples",
    "new": "node ./build/new.js",
    "build:file": "node build/build-entry.js && node build/version.js",
    "build:theme": "node build/gen-cssfile && gulp build --gulpfile packages/theme-chalk/gulpfile.js && cp-cli packages/theme-chalk/lib lib/theme-chalk",
    "lib": "vue-cli-service build --target lib --name yx --dest lib src/index.js",
    "pub": "npm run build:file && npm run lib && npm run build:theme && node ./build/gen-indices.js"
  },
  "main": "lib/yx.umd.min.js",
  "typings": "types/index.d.ts",
  "unpkg": "lib/index.js",
  "style": "lib/theme-chalk/index.css",
  "dependencies": {
    "core-js": "^3.6.5",
    "throttle-debounce": "1.0.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "algoliasearch": "3.24.5",
    "babel-eslint": "^10.1.0",
    "cp-cli": "^2.0.0",
    "element-ui": "^2.14.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "file-save": "^0.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "highlight.js": "^9.3.0",
    "json-templater": "1.0.4",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "svg-sprite-loader": "^5.0.0",
    "transliteration": "1.1.11",
    "uppercamelcase": "^3.0.0",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "standard/no-callback-literal": 0,
      "vue/comment-directive": 0,
      "vue/valid-template-root": 0,
      "no-trailing-spaces": 0,
      "no-prototype-builtins": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Sauron220/yx.git"
  },
  "license": "MIT"
}
